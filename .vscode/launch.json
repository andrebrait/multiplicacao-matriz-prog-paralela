    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch BLAS",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/benchmark-blas",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "preLaunchTask": "build-debug",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) Attach BLAS",
            "type": "cppdbg",
            "request": "attach",
            "program": "${workspaceRoot}/benchmark-blas",
            "processId": "${command:pickProcess}",
            "MIMode": "gdb"
        },
